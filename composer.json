{
  "name": "minchao/mitake-php",
  "description": "A Mitake SMS library for PHP (Unofficial)",
  "keyword": ["mitake", "sms", "api"],
  "homepage": "https://github.com/minchao/mitake-php",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Mitake\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mitake\\Tests\\": "tests/"
    }
  }
}
